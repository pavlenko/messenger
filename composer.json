{
  "name": "pe/component-messenger",
  "type": "library",
  "description": "messenger",
  "license": "MIT",
  "minimum-stability": "stable",
  "config": {
    "sort-packages": true
  },
  "require": {
    "php": "^7.4",
    "ext-json": "*",
    "ext-simplexml": "*",
    "psr/http-message": "^1.1",
    "psr/log": "^1.1"
  },
  "require-dev": {
    "php-mock/php-mock": "^2.1",
    "php-mock/php-mock-phpunit": "^2.5",
    "phpunit/phpunit": "^9.3"
  },
  "autoload": {
    "psr-4": {
      "PE\\Component\\Messenger\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PE\\Component\\Messenger\\Tests\\": "tests/"
    }
  }
}
